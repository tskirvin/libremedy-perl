#########################################################################
### Remedy Schema and Status Maps
#########################################################################
# Maps tables to their underlying schema, and status fields to their underlying 
# human-readable descriptions.  
#
# $Id: schema 4522 2008-06-27 17:53:33Z tskirvin $

our %SCHEMA_MAP = (
    'Audit'     => $schema_audit,
    'BC'        => $schema_bc,
    'Group'     => $schema_group,
    'HDSupport' => $schema_hd_support,
    'HDWorkLog' => $schema_hd_worklog,
    'HelpDesk'  => $schema_helpdesk,
    'TicketGen' => $schema_ticketgen,
    'User'      => $schema_user,
);

our %STATUS_MAP = (
    'Impact'    => $map_impact,
    'Priority'  => $map_priority,
    'Reason'    => $map_reason,
    'Status'    => $map_status,
    'Urgency'   => $map_urgency,
    'WorkLog'   => $map_worklog,
);

##############################################################################
### Schema Maps
##############################################################################

## Audit Logs - tracks who made what changes to the database.  Incomplete, 
## but good enough for our use.
our $schema_audit = {
      1 => "Entry ID",
      3 => "Create Time",
    450 => "EID",
    455 => "Change Person",
    456 => "Changed Fields",
    459 => "Changed Data",
};

